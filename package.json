{
    "name": "connect",
    "version": "1.0.50",
    "scripts": {
        "dev": "concurrently \"bun run dev:client\" \"bun run dev:sass\" \"bun run dev:server\"",
        "dev:server": "bun run --watch ./src/index.tsx",
        "dev:client": "bun build.ts",
        "dev:sass": "sass --watch src/sass:public/css",
        "migrate": "bun x prisma migrate dev --name init",
        "compile-rs": "cd file-handle && cargo build --release"
    },
    "dependencies": {
        "@elysiajs/cookie": "^0.8.0",
        "@elysiajs/jwt": "^1.3.2",
        "@elysiajs/static": "^1.3.0",
        "@elysiajs/stream": "^1.1.0",
        "@ffmpeg/ffmpeg": "^0.12.15",
        "@lottiefiles/dotlottie-web": "^0.50.1",
        "@motionone/solid": "^10.16.4",
        "@prisma/client": "6.14.0",
        "@solidjs/router": "^0.15.3",
        "elysia": "latest",
        "solid-js": "^1.9.7",
        "sqlite3": "^5.1.7"
    },
    "devDependencies": {
        "bun-types": "latest",
        "esbuild": "^0.25.8",
        "esbuild-plugin-solid": "^0.6.0",
        "concurrently": "^9.2.1",
        "prisma": "6.14.0"
    },
    "module": "src/index.js"
}

{
    "name": "connect",
    "version": "1.0.50",
    "scripts": {
        "dev": "concurrently \"bun run dev:client\" \"bun run dev:sass\" \"bun run dev:server\"",
        "dev:server": "bun run ./src/index.tsx",
        "dev:client": "bun build.ts",
        "dev:sass": "sass --watch src/sass:public/css"
    },
    "dependencies": {
        "@elysiajs/static": "^1.3.0",
        "@elysiajs/stream": "^1.1.0",
        "@ffmpeg/ffmpeg": "^0.12.15",
        "@lottiefiles/dotlottie-web": "^0.49.0",
        "@solidjs/router": "^0.15.3",
        "elysia": "latest",
        "fluent-ffmpeg": "^2.1.3",
        "solid-js": "^1.9.7"
    },
    "devDependencies": {
        "@types/fluent-ffmpeg": "^2.1.27",
        "bun-types": "latest",
        "concurrently": "^9.2.0",
        "esbuild": "^0.25.8",
        "esbuild-plugin-solid": "^0.6.0"
    },
    "module": "src/index.js"
}
